[user]
	name = Ian Langworth
[core]
	editor = vim
	pager = less -FRX
	excludesfile = ~/.gitignore
[github]
	user = statico
[alias]
	up = pull origin
	st = status
	co = checkout
	ci = commit
	civ = commit -v
	cia = commit -a
	civa = commit -v -a
	cim = commit -m
	b = branch
	sta = stash
	rewind-one = reset --hard HEAD^
	svnprep = rebase -i git-svn
	svnup = svn rebase
	svnci = svn dcommit
	last = log -1
	newb = checkout -b
	bnew = checkout -b
[color]
	diff = auto
	status = auto
	branch = auto
